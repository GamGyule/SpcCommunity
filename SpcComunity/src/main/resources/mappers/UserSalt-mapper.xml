<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spc.comunity.dao.MybatisUserSaltRepository">
	
	<select id="findAll" resultType="com.spc.comunity.dto.UserSaltDto">
		select * from TBL_USER_INFO
	</select>
	
	<select id="getUserSalt" resultType="string" parameterType="string">
		select SALT from TBL_SALT where ID = #{userId}
	</select>
	
</mapper>