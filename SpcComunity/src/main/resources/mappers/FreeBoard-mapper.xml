<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spc.comunity.dao.MybatisFreeBoardRepository">


	<select id="findPage"
		resultType="com.spc.comunity.dto.FreeBoardDto" parameterType="Integer">
		select * from TBL_FREE_BOARD order by idx desc limit ${page}, 20;
	</select>


</mapper>